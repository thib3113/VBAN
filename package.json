{
  "name": "vban",
  "version": "1.2.4",
  "description": "Node VBAN implementation",
  "main": "lib/index.js",
  "devDependencies": {
    "@tsconfig/node14": "14.1.0",
    "@types/jest": "29.5.10",
    "@types/node": "20.10.3",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "dotenv": "16.3.1",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.1",
